{
	"name": "personal-site",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"prepare": "npm run smui-theme"
	},
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.3",
		"@smui/drawer": "^7.0.0-beta.3",
		"@smui/icon-button": "^7.0.0-beta.3",
		"@smui/list": "^7.0.0-beta.3",
		"@smui/paper": "^7.0.0-beta.3",
		"@smui/tab": "^7.0.0-beta.3",
		"@smui/tab-bar": "^7.0.0-beta.3",
		"@smui/top-app-bar": "^7.0.0-beta.3",
		"@sveltejs/adapter-auto": "^1.0.0-next.90",
		"@sveltejs/kit": "^1.0.0-next.587",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"smui-theme": "^7.0.0-beta.3",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-copy-to-clipboard": "^0.2.5",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module"
}
